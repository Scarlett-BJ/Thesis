@article{Minami,
	Author = {Minami, Haruka and Tashiro, Katsuhisa and Okada, Atsumasa and Hirata, Nobue and Yamaguchi, Tomoko and Takayama, Kazuo and Mizuguchi, Hiroyuki and Kawabata, Kenji},
