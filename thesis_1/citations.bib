@article{Sandoval,
	Author = {Sandoval, Ignacio V. and Martı́nez-Arca, Sonia and Valdueza, Julio and Palacios, Silvia and Holman, Geoffrey D.},
	Date = {2000/12/22},
	Date-Added = {2017-07-26 12:31:20 +0000},
	Date-Modified = {2017-07-26 12:31:30 +0000},
	Journal = {Journal of Biological Chemistry},
	M3 = {10.1074/jbc.M006261200},
	Month = {12},
	Number = {51},
	Pages = {39874--39885},
	Title = {Distinct Reading of Different Structural Determinants Modulates the Dileucine-mediated Transport Steps of the Lysosomal Membrane Protein LIMPII and the Insulin-sensitive Glucose Transporter {GLUT}4.},
	Ty = {JOUR},
	Url = {http://www.jbc.org/content/275/51/39874.abstract N2 - Leucine-based motifs mediate the sorting of membrane proteins at such cellular sites as the trans-Golgi network, endosomes, and plasma membrane. A Leu paired with a second Leu, Ile, or Met, while itself lacking the ability to mediate transport, is the key structural feature in these motifs. Here we have studied the structural differences between the leucine-based motifs contained in the COOH tails of LIMPII and GLUT4, two membrane proteins that are transported through the secretory pathway and are targeted to lysosomes (1-3) and to a perinuclear compartment adjacent to the Golgi complex (4), respectively. LIMPII and GLUT4 display negatively (Asp470/Glu471) and positively (Arg484/Arg485) charged residues, respectively, at positions −4 and −5 upstream from the critical Leu residue. The change in the charge sign of residues −4 and −5 results in missorting of LIMPII and GLUT4. We note that the acidic Glu residue at position −4 is critical for efficient intracellular sorting of LIMPII to lysosomes, but is dispensable for its surface internalization by endocytosis. Efficient intracellular sorting and endocytosis of GLUT4 require an Arg pair between positions −4 and −7. These results are consistent with the existence of distinct leucine-based motifs and provide evidence of their different readings at different cellular sites.},
	Volume = {275},
	Year = {2000},
	Bdsk-Url-1 = {http://www.jbc.org/content/275/51/39874.abstract%20N2%20-%20Leucine-based%20motifs%20mediate%20the%20sorting%20of%20membrane%20proteins%20at%20such%20cellular%20sites%20as%20the%20trans-Golgi%20network,%20endosomes,%20and%20plasma%20membrane.%20A%20Leu%20paired%20with%20a%20second%20Leu,%20Ile,%20or%20Met,%20while%20itself%20lacking%20the%20ability%20to%20mediate%20transport,%20is%20the%20key%20structural%20feature%20in%20these%20motifs.%20Here%20we%20have%20studied%20the%20structural%20differences%20between%20the%20leucine-based%20motifs%20contained%20in%20the%20COOH%20tails%20of%20LIMPII%20and%20GLUT4,%20two%20membrane%20proteins%20that%20are%20transported%20through%20the%20secretory%20pathway%20and%20are%20targeted%20to%20lysosomes%20(1-3)%20and%20to%20a%20perinuclear%20compartment%20adjacent%20to%20the%20Golgi%20complex%20(4),%20respectively.%20LIMPII%20and%20GLUT4%20display%20negatively%20(Asp470/Glu471)%20and%20positively%20(Arg484/Arg485)%20charged%20residues,%20respectively,%20at%20positions%20%E2%88%924%20and%20%E2%88%925%20upstream%20from%20the%20critical%20Leu%20residue.%20The%20change%20in%20the%20charge%20sign%20of%20residues%20%E2%88%924%20and%20%E2%88%925%20results%20in%20missorting%20of%20LIMPII%20and%20GLUT4.%20We%20note%20that%20the%20acidic%20Glu%20residue%20at%20position%20%E2%88%924%20is%20critical%20for%20efficient%20intracellular%20sorting%20of%20LIMPII%20to%20lysosomes,%20but%20is%20dispensable%20for%20its%20surface%20internalization%20by%20endocytosis.%20Efficient%20intracellular%20sorting%20and%20endocytosis%20of%20GLUT4%20require%20an%20Arg%20pair%20between%20positions%20%E2%88%924%20and%20%E2%88%927.%20These%20results%20are%20consistent%20with%20the%20existence%20of%20distinct%20leucine-based%20motifs%20and%20provide%20evidence%20of%20their%20different%20readings%20at%20different%20cellular%20sites.}}

@article{Bonifacino,
	Abstract = {Sorting of transmembrane proteins to endosomes and lysosomes is mediated by signals present within the cytosolic domains of the proteins. Most signals consist of short, linear sequences of amino acid residues. Some signals are referred to as tyrosine-based sorting signals and conform to the NPXY or YXXO consensus motifs. Other signals known as dileucine-based signals fit [DE]XXXL[LI] or DXXLL consensus motifs. All of these signals are recognized by components of protein coats peripherally associated with the cytosolic face of membranes. YXXO and [DE]XXXL[LI] signals are recognized with characteristic fine specificity by the adaptor protein (AP) complexes AP-1, AP-2, AP-3, and AP-4, whereas DXXLL signals are recognized by another family of adaptors known as GGAs. Several proteins, including clathrin, AP-2, and Dab2, have been proposed to function as recognition proteins for NPXY signals. YXXO and DXXLL signals bind in an extended conformation to the mu2 subunit of AP-2 and the VHS domain of the GGAs, respectively. Phosphorylation events regulate signal recognition. In addition to peptide motifs, ubiquitination of cytosolic lysine residues also serves as a signal for sorting at various stages of the endosomal-lysosomal system. Conjugated ubiquitin is recognized by UIM, UBA, or UBC domains present within many components of the internalization and lysosomal targeting machinery. This complex array of signals and recognition proteins ensures the dynamic but accurate distribution of transmembrane proteins to different compartments of the endosomal-lysosomal system.},
	Address = {Cell Biology and Metabolism Branch, National Institute of Child Health and Human Development, National Institutes of Health, Bethesda, Maryland 20892, USA. juan@helix.nih.gov},
