@article{Daro,
	Author = {Daro, E and van der Sluijs, P and Galli, T and Mellman, I},
	Date = {1996/09/03},
	Date-Added = {2017-07-22 20:22:56 +0000},
	Date-Modified = {2017-07-22 20:23:03 +0000},
	Journal = {Proceedings of the National Academy of Sciences},
	Month = {09},
	Number = {18},
	Pages = {9559--9564},
	Title = {Rab4 and cellubrevin define different early endosome populations on the pathway of transferrin receptor recycling.},
	Ty = {JOUR},
	Url = {http://www.pnas.org/content/93/18/9559.abstract N2 - During receptor mediated endocytosis, at least a fraction of recycling cargo typically accumulates in a pericentriolar cluster of tubules and vesicles. However, it is not clear if these endosomal structures are biochemically distinct from the early endosomes from which they are derived. To better characterize this pericentriolar endosome population, we determined the distribution of two endogenous proteins known to be functionally involved in receptor recycling [Rab4, cellubrevin (Cbvn)] relative to the distribution of a recycling ligand [transferrin (Tfn)] as it traversed the endocytic pathway. Shortly after internalization, Tfn entered a population of early endosomes that contained both Rab4 and Cbvn, demonstrated by triple label immunofluorescence confocal microscopy. Tfn then accumulated in the pericentriolar cluster of recycling vesicles (RVs). However, although these pericentriolar endosomes contained Cbvn, they were strikingly depleted of Rab4. The ability of internalized Tfn to reach the Rab4-negative population was not blocked by nocodazole, although the characteristic pericentriolar location of the population was not maintained in the absence of microtubules. Similarly, Rab4-positive and -negative populations remained distinct in cells treated with brefeldin A, with only Rab4-positive elements exhibiting the extended tubular morphology induced by the drug. Thus, at least with respect to Rab4 distribution, the pathway of Tfn receptor recycling consists of at least two biochemically and functionally distinct populations of endosomes, a Rab4-positive population of early endosomes to which incoming Tfn is initially delivered and a Rab4-negative population of recycling vesicles that transiently accumulates Tfn on its route back to the plasma membrane.},
	Volume = {93},
	Year = {1996},
	Bdsk-Url-1 = {http://www.pnas.org/content/93/18/9559.abstract%20N2%20-%20During%20receptor%20mediated%20endocytosis,%20at%20least%20a%20fraction%20of%20recycling%20cargo%20typically%20accumulates%20in%20a%20pericentriolar%20cluster%20of%20tubules%20and%20vesicles.%20However,%20it%20is%20not%20clear%20if%20these%20endosomal%20structures%20are%20biochemically%20distinct%20from%20the%20early%20endosomes%20from%20which%20they%20are%20derived.%20To%20better%20characterize%20this%20pericentriolar%20endosome%20population,%20we%20determined%20the%20distribution%20of%20two%20endogenous%20proteins%20known%20to%20be%20functionally%20involved%20in%20receptor%20recycling%20%5BRab4,%20cellubrevin%20(Cbvn)%5D%20relative%20to%20the%20distribution%20of%20a%20recycling%20ligand%20%5Btransferrin%20(Tfn)%5D%20as%20it%20traversed%20the%20endocytic%20pathway.%20Shortly%20after%20internalization,%20Tfn%20entered%20a%20population%20of%20early%20endosomes%20that%20contained%20both%20Rab4%20and%20Cbvn,%20demonstrated%20by%20triple%20label%20immunofluorescence%20confocal%20microscopy.%20Tfn%20then%20accumulated%20in%20the%20pericentriolar%20cluster%20of%20recycling%20vesicles%20(RVs).%20However,%20although%20these%20pericentriolar%20endosomes%20contained%20Cbvn,%20they%20were%20strikingly%20depleted%20of%20Rab4.%20The%20ability%20of%20internalized%20Tfn%20to%20reach%20the%20Rab4-negative%20population%20was%20not%20blocked%20by%20nocodazole,%20although%20the%20characteristic%20pericentriolar%20location%20of%20the%20population%20was%20not%20maintained%20in%20the%20absence%20of%20microtubules.%20Similarly,%20Rab4-positive%20and%20-negative%20populations%20remained%20distinct%20in%20cells%20treated%20with%20brefeldin%20A,%20with%20only%20Rab4-positive%20elements%20exhibiting%20the%20extended%20tubular%20morphology%20induced%20by%20the%20drug.%20Thus,%20at%20least%20with%20respect%20to%20Rab4%20distribution,%20the%20pathway%20of%20Tfn%20receptor%20recycling%20consists%20of%20at%20least%20two%20biochemically%20and%20functionally%20distinct%20populations%20of%20endosomes,%20a%20Rab4-positive%20population%20of%20early%20endosomes%20to%20which%20incoming%20Tfn%20is%20initially%20delivered%20and%20a%20Rab4-negative%20population%20of%20recycling%20vesicles%20that%20transiently%20accumulates%20Tfn%20on%20its%20route%20back%20to%20the%20plasma%20membrane.}}

@article{Byrd,
	Author = {Byrd, James C. and Park, Jung H. Y. and Schaffer, Beverly S. and Garmroudi, Farideh and MacDonald, Richard G.},
	Date = {2000/06/23},
	Date-Added = {2017-07-22 20:14:50 +0000},
	Date-Modified = {2017-07-22 20:14:58 +0000},
	Journal = {Journal of Biological Chemistry},
	M3 = {10.1074/jbc.M001273200},
	Month = {06},
	Number = {25},
	Pages = {18647--18656},
	Title = {Dimerization of the Insulin-like Growth Factor II/Mannose 6-Phosphate Receptor.},
	Ty = {JOUR},
	Url = {http://www.jbc.org/content/275/25/18647.abstract N2 - The insulin-like growth factor II/mannose 6-phosphate receptor (IGF2R) interacts with lysosomal enzymes through two binding domains in its extracytoplasmic domain. We report in the accompanying article (Byrd, J. C., and MacDonald, R. G. (2000) J. Biol. Chem. 275, 18638–18646) that only one of the two extracytoplasmic mannose 6-phosphate (Man-6-P) binding domains is necessary for high affinity Man-6-P ligand binding, suggesting that, like the cation-dependent Man-6-P receptor, oligomerization of the IGF2R contributes to high affinity interaction with lysosomal enzymes. In the present study, we have directly characterized both naturally occurring and engineered forms of the IGF2R for their ability to form oligomeric structures. Whereas gel filtration chromatography suggested that purified bovine IGF2R species exist in a monomeric form, native gel electrophoresis allowed for the separation of dimeric and monomeric forms of the receptors with distinct phosphomannosyl ligand binding characteristics. The ability of the IGF2R to form oligomeric complexes was confirmed and localized to the extracytoplasmic domain through the use of epitope-tagged soluble IGF2R constructs bearing deletions of the transmembrane and cytoplasmic domains. Finally, chimeric receptors were engineered containing the extracytoplasmic and transmembrane domains of the IGF2R fused to the cytoplasmic domain of the epidermal growth factor receptor with which dimerization of the chimeras could be monitored by measuring autophosphorylation. Collectively, these results show that the IGF2R is capable of forming oligomeric complexes, most likely dimers, in the absence of Man-6-P ligands.},
	Volume = {275},
	Year = {2000},
	Bdsk-Url-1 = {http://www.jbc.org/content/275/25/18647.abstract%20N2%20-%20The%20insulin-like%20growth%20factor%20II/mannose%206-phosphate%20receptor%20(IGF2R)%20interacts%20with%20lysosomal%20enzymes%20through%20two%20binding%20domains%20in%20its%20extracytoplasmic%20domain.%20We%20report%20in%20the%20accompanying%20article%20(Byrd,%20J.%20C.,%20and%20MacDonald,%20R.%20G.%20(2000)%20J.%20Biol.%20Chem.%20275,%2018638%E2%80%9318646)%20that%20only%20one%20of%20the%20two%20extracytoplasmic%20mannose%206-phosphate%20(Man-6-P)%20binding%20domains%20is%20necessary%20for%20high%20affinity%20Man-6-P%20ligand%20binding,%20suggesting%20that,%20like%20the%20cation-dependent%20Man-6-P%20receptor,%20oligomerization%20of%20the%20IGF2R%20contributes%20to%20high%20affinity%20interaction%20with%20lysosomal%20enzymes.%20In%20the%20present%20study,%20we%20have%20directly%20characterized%20both%20naturally%20occurring%20and%20engineered%20forms%20of%20the%20IGF2R%20for%20their%20ability%20to%20form%20oligomeric%20structures.%20Whereas%20gel%20filtration%20chromatography%20suggested%20that%20purified%20bovine%20IGF2R%20species%20exist%20in%20a%20monomeric%20form,%20native%20gel%20electrophoresis%20allowed%20for%20the%20separation%20of%20dimeric%20and%20monomeric%20forms%20of%20the%20receptors%20with%20distinct%20phosphomannosyl%20ligand%20binding%20characteristics.%20The%20ability%20of%20the%20IGF2R%20to%20form%20oligomeric%20complexes%20was%20confirmed%20and%20localized%20to%20the%20extracytoplasmic%20domain%20through%20the%20use%20of%20epitope-tagged%20soluble%20IGF2R%20constructs%20bearing%20deletions%20of%20the%20transmembrane%20and%20cytoplasmic%20domains.%20Finally,%20chimeric%20receptors%20were%20engineered%20containing%20the%20extracytoplasmic%20and%20transmembrane%20domains%20of%20the%20IGF2R%20fused%20to%20the%20cytoplasmic%20domain%20of%20the%20epidermal%20growth%20factor%20receptor%20with%20which%20dimerization%20of%20the%20chimeras%20could%20be%20monitored%20by%20measuring%20autophosphorylation.%20Collectively,%20these%20results%20show%20that%20the%20IGF2R%20is%20capable%20of%20forming%20oligomeric%20complexes,%20most%20likely%20dimers,%20in%20the%20absence%20of%20Man-6-P%20ligands.}}

@article{Hirst,
	Author = {Hirst, Jennifer and Edgar, James R. and Borner, Georg H. H. and Li, Sam and Sahlender, Daniela A. and Antrobus, Robin and Robinson, Margaret S.},
	Date = {2015/09/01},
	Date-Added = {2017-07-22 19:49:21 +0000},
	Date-Modified = {2017-07-22 19:49:39 +0000},
	Journal = {Molecular Biology of the Cell},
	M3 = {10.1091/mbc.E15-04-0245},
	Month = {09},
	Number = {17},
	Pages = {3085--3103},
	Title = {Contributions of epsin{R} and gadkin to clathrin-mediated intracellular trafficking.},
	Ty = {JOUR},
	Url = {http://www.molbiolcell.org/content/26/17/3085.abstract N2 - The precise functions of most of the proteins that participate in clathrin-mediated intracellular trafficking are unknown. We investigated two such proteins, epsinR and gadkin, using the knocksideways method, which rapidly depletes proteins from the available pool by trapping them onto mitochondria. Although epsinR is known to be an N-ethylmaleimide–sensitive factor attachment protein receptor (SNARE)-specific adaptor, the epsinR knocksideways blocked the production of the entire population of intracellular clathrin-coated vesicles (CCVs), suggesting a more global function. Using the epsinR knocksideways data, we were able to estimate the copy number of all major intracellular CCV proteins. Both sides of the vesicle are densely covered, indicating that CCVs sort their cargo by molecular crowding. Trapping of gadkin onto mitochondria also blocked the production of intracellular CCVs but by a different mechanism: vesicles became cross-linked to mitochondria and pulled out toward the cell periphery. Both phenotypes provide new insights into the regulation of intracellular CCV formation, which could not have been found using more conventional approaches.},
	Volume = {26},
	Year = {2015},
	Bdsk-Url-1 = {http://www.molbiolcell.org/content/26/17/3085.abstract%20N2%20-%20The%20precise%20functions%20of%20most%20of%20the%20proteins%20that%20participate%20in%20clathrin-mediated%20intracellular%20trafficking%20are%20unknown.%20We%20investigated%20two%20such%20proteins,%20epsinR%20and%20gadkin,%20using%20the%20knocksideways%20method,%20which%20rapidly%20depletes%20proteins%20from%20the%20available%20pool%20by%20trapping%20them%20onto%20mitochondria.%20Although%20epsinR%20is%20known%20to%20be%20an%20N-ethylmaleimide%E2%80%93sensitive%20factor%20attachment%20protein%20receptor%20(SNARE)-specific%20adaptor,%20the%20epsinR%20knocksideways%20blocked%20the%20production%20of%20the%20entire%20population%20of%20intracellular%20clathrin-coated%20vesicles%20(CCVs),%20suggesting%20a%20more%20global%20function.%20Using%20the%20epsinR%20knocksideways%20data,%20we%20were%20able%20to%20estimate%20the%20copy%20number%20of%20all%20major%20intracellular%20CCV%20proteins.%20Both%20sides%20of%20the%20vesicle%20are%20densely%20covered,%20indicating%20that%20CCVs%20sort%20their%20cargo%20by%20molecular%20crowding.%20Trapping%20of%20gadkin%20onto%20mitochondria%20also%20blocked%20the%20production%20of%20intracellular%20CCVs%20but%20by%20a%20different%20mechanism:%20vesicles%20became%20cross-linked%20to%20mitochondria%20and%20pulled%20out%20toward%20the%20cell%20periphery.%20Both%20phenotypes%20provide%20new%20insights%20into%20the%20regulation%20of%20intracellular%20CCV%20formation,%20which%20could%20not%20have%20been%20found%20using%20more%20conventional%20approaches.}}

