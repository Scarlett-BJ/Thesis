@article{Koliwer,
	Author = {Koliwer, Judith and Park, Minjong and Bauch, Carola and von Zastrow, Mark and Kreienkamp, Hans-J{\"u}rgen},
	Date = {2015/03/06},
	Date-Added = {2017-07-29 18:12:33 +0000},
	Date-Modified = {2017-07-29 18:12:49 +0000},
	Journal = {Journal of Biological Chemistry},
	M3 = {10.1074/jbc.M114.605725},
	Month = {03},
	Number = {10},
	Pages = {6120--6129},
	Title = {The {G}olgi-associated {PDZ} Domain Protein {PIST/GOPC} Stabilizes the β1-Adrenergic Receptor in Intracellular Compartments after Internalization.},
	Ty = {JOUR},
	Url = {http://www.jbc.org/content/290/10/6120.abstract N2 - Many G-protein-coupled receptors carry C-terminal ligand motifs for PSD-95/discs large/ZO-1 (PDZ) domains; via interaction with PDZ domain-containing scaffold proteins, this allows for integration of receptors into signaling complexes. However, the presence of PDZ domain proteins attached to intracellular membranes suggests that PDZ-type interactions may also contribute to subcellular sorting of receptors. The protein interacting specifically with Tc10 (PIST; also known as GOPC) is a trans-Golgi-associated protein that interacts through its single PDZ domain with a variety of cell surface receptors. Here we show that PIST controls trafficking of the interacting β1-adrenergic receptor both in the anterograde, biosynthetic pathway and during postendocytic recycling. Overexpression and knockdown experiments show that PIST leads to retention of the receptor in the trans-Golgi network (TGN), to the effect that overexpressed PIST reduces activation of the MAPK pathway by β1-adrenergic receptor (β1AR) agonists. Receptors can be released from retention in the TGN by coexpression of the plasma membrane-associated scaffold PSD-95, which allows for transport of receptors to the plasma membrane. Stimulation of β1 receptors and activation of the cAMP pathway lead to relocation of PIST from the TGN to an endosome-like compartment. Here PIST colocalizes with SNX1 and the internalized β1AR and protects endocytosed receptors from lysosomal degradation. In agreement, β1AR levels are decreased in hippocampi of PIST-deficient mice. Our data suggest that PIST contributes to the fine-tuning of β1AR sorting both during biosynthetic and postendocytic trafficking.},
	Volume = {290},
	Year = {2015},
	Bdsk-Url-1 = {http://www.jbc.org/content/290/10/6120.abstract%20N2%20-%20Many%20G-protein-coupled%20receptors%20carry%20C-terminal%20ligand%20motifs%20for%20PSD-95/discs%20large/ZO-1%20(PDZ)%20domains;%20via%20interaction%20with%20PDZ%20domain-containing%20scaffold%20proteins,%20this%20allows%20for%20integration%20of%20receptors%20into%20signaling%20complexes.%20However,%20the%20presence%20of%20PDZ%20domain%20proteins%20attached%20to%20intracellular%20membranes%20suggests%20that%20PDZ-type%20interactions%20may%20also%20contribute%20to%20subcellular%20sorting%20of%20receptors.%20The%20protein%20interacting%20specifically%20with%20Tc10%20(PIST;%20also%20known%20as%20GOPC)%20is%20a%20trans-Golgi-associated%20protein%20that%20interacts%20through%20its%20single%20PDZ%20domain%20with%20a%20variety%20of%20cell%20surface%20receptors.%20Here%20we%20show%20that%20PIST%20controls%20trafficking%20of%20the%20interacting%20%CE%B21-adrenergic%20receptor%20both%20in%20the%20anterograde,%20biosynthetic%20pathway%20and%20during%20postendocytic%20recycling.%20Overexpression%20and%20knockdown%20experiments%20show%20that%20PIST%20leads%20to%20retention%20of%20the%20receptor%20in%20the%20trans-Golgi%20network%20(TGN),%20to%20the%20effect%20that%20overexpressed%20PIST%20reduces%20activation%20of%20the%20MAPK%20pathway%20by%20%CE%B21-adrenergic%20receptor%20(%CE%B21AR)%20agonists.%20Receptors%20can%20be%20released%20from%20retention%20in%20the%20TGN%20by%20coexpression%20of%20the%20plasma%20membrane-associated%20scaffold%20PSD-95,%20which%20allows%20for%20transport%20of%20receptors%20to%20the%20plasma%20membrane.%20Stimulation%20of%20%CE%B21%20receptors%20and%20activation%20of%20the%20cAMP%20pathway%20lead%20to%20relocation%20of%20PIST%20from%20the%20TGN%20to%20an%20endosome-like%20compartment.%20Here%20PIST%20colocalizes%20with%20SNX1%20and%20the%20internalized%20%CE%B21AR%20and%20protects%20endocytosed%20receptors%20from%20lysosomal%20degradation.%20In%20agreement,%20%CE%B21AR%20levels%20are%20decreased%20in%20hippocampi%20of%20PIST-deficient%20mice.%20Our%20data%20suggest%20that%20PIST%20contributes%20to%20the%20fine-tuning%20of%20%CE%B21AR%20sorting%20both%20during%20biosynthetic%20and%20postendocytic%20trafficking.}}

