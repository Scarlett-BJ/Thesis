@article{Lo,
	Author = {Lo, Bernice and Zhang, Kejian and Lu, Wei and Zheng, Lixin and Zhang, Qian and Kanellopoulou, Chrysi and Zhang, Yu and Liu, Zhiduo and Fritz, Jill M. and Marsh, Rebecca and Husami, Ammar and Kissell, Diane and Nortman, Shannon and Chaturvedi, Vijaya and Haines, Hilary and Young, Lisa R. and Mo, Jun and Filipovich, Alexandra H. and Bleesing, Jack J. and Mustillo, Peter and Stephens, Michael and Rueda, Cesar M. and Chougnet, Claire A. and Hoebe, Kasper and McElwee, Joshua and Hughes, Jason D. and Karakoc-Aydiner, Elif and Matthews, Helen F. and Price, Susan and Su, Helen C. and Rao, V. Koneti and Lenardo, Michael J. and Jordan, Michael B.},
	Date = {2015/07/23},
	Date-Added = {2017-07-29 23:07:03 +0000},
	Date-Modified = {2017-07-29 23:07:03 +0000},
	Journal = {Science},
	Journal1 = {Science},
	Lp = {440},
	M3 = {10.1126/science.aaa1663},
	Month = {07},
	N2 = {Typically in translational research, a discovery in cell or molecular biology is later exploited to improve patient care. Occasionally, information flows in the opposite direction. Lo et al. found that patients with an autoimmune disorder caused by deficiency of a protein called LRBA responded dramatically to the drug abatacept (see the Perspective by Sansom). Abatacept contains a segment of a potent inhibitory immune receptor, CTLA4. Experiments prompted by this observation revealed the relationship between the two proteins: LRBA controls the intracellular trafficking and degradation of CTLA4. This information may further improve patient care, because other clinically approved drugs have the desired mechanism of action with potentially fewer side effects.Science, this issue p. 436; see also p. 377Mutations in the LRBA gene (encoding the lipopolysaccharide-responsive and beige-like anchor protein) cause a syndrome of autoimmunity, lymphoproliferation, and humoral immune deficiency. The biological role of LRBA in immunologic disease is unknown. We found that patients with LRBA deficiency manifested a dramatic and sustained improvement in response to abatacept, a CTLA4 (cytotoxic T lymphocyte antigen-4)--immunoglobulin fusion drug. Clinical responses and homology of LRBA to proteins controlling intracellular trafficking led us to hypothesize that it regulates CTLA4, a potent inhibitory immune receptor. We found that LRBA colocalized with CTLA4 in endosomal vesicles and that LRBA deficiency or knockdown increased CTLA4 turnover, which resulted in reduced levels of CTLA4 protein in FoxP3+ regulatory and activated conventional T cells. In LRBA-deficient cells, inhibition of lysosome degradation with chloroquine prevented CTLA4 loss. These findings elucidate a mechanism for CTLA4 trafficking and control of immune responses and suggest therapies for diseases involving the CTLA4 pathway.},
	Number = {6246},
	Pages = {436},
	Title = {Patients with LRBA deficiency show CTLA4 loss and immune dysregulation responsive to abatacept therapy},
	Ty = {JOUR},
	Url = {http://science.sciencemag.org/content/349/6246/436.abstract},
	Volume = {349},
	Year = {2015},
	Bdsk-Url-1 = {http://science.sciencemag.org/content/349/6246/436.abstract}}

@article{Tanabe,
	Author = {Tanabe, Kenji and Torii, Tetsuo and Natsume, Waka and Braesch-Andersen, Sten and Watanabe, Toshio and Satake, Masanobu},
	Date = {2005/04/01},
	Date-Added = {2017-07-29 22:59:07 +0000},
	Date-Modified = {2017-07-29 22:59:07 +0000},
	Journal = {Molecular Biology of the Cell},
	M3 = {10.1091/mbc.E04-08-0683},
	Month = {04},
	Number = {4},
	Pages = {1617--1628},
	Title = {A Novel GTPase-activating Protein for ARF6 Directly Interacts with Clathrin and Regulates Clathrin-dependent Endocytosis},
	Ty = {JOUR},
	Url = {http://www.molbiolcell.org/content/16/4/1617.abstract N2 - ADP-ribosylation factor 6 (Arf6) is a small-GTPase that regulates the membrane trafficking between the plasma membrane and endosome. It is also involved in the reorganization of the actin cytoskeleton. GTPase-activating protein (GAP) is a critical regulator of Arf function as it inactivates Arf. Here, we identified a novel species of GAP denoted as SMAP1 that preferentially acts on Arf6. Although overexpression of SMAP1 did not alter the subcellular distribution of the actin cytoskeleton, it did block the endocytosis of transferrin receptors. Knock down of endogenous SMAP1 also abolished transferrin internalization, which confirms that SMAP1 is needed for this endocytic process. SMAP1 overexpression had no effect on clathrin-independent endocytosis, however. Intriguingly, SMAP1 binds directly to the clathrin heavy chain via its clathrin-box and mutation studies revealed that its GAP domain and clathrin-box both contribute to the role SMAP1 plays in clathrin-dependent endocytosis. These observations suggest that SMAP1 may be an Arf6GAP that specifically regulates one of the multiple functions of Arf6, namely, clathrin-dependent endocytosis, and that it does so by binding directly to clathrin.},
	Volume = {16},
	Year = {2005},
	Bdsk-Url-1 = {http://www.molbiolcell.org/content/16/4/1617.abstract%20N2%20-%20ADP-ribosylation%20factor%206%20(Arf6)%20is%20a%20small-GTPase%20that%20regulates%20the%20membrane%20trafficking%20between%20the%20plasma%20membrane%20and%20endosome.%20It%20is%20also%20involved%20in%20the%20reorganization%20of%20the%20actin%20cytoskeleton.%20GTPase-activating%20protein%20(GAP)%20is%20a%20critical%20regulator%20of%20Arf%20function%20as%20it%20inactivates%20Arf.%20Here,%20we%20identified%20a%20novel%20species%20of%20GAP%20denoted%20as%20SMAP1%20that%20preferentially%20acts%20on%20Arf6.%20Although%20overexpression%20of%20SMAP1%20did%20not%20alter%20the%20subcellular%20distribution%20of%20the%20actin%20cytoskeleton,%20it%20did%20block%20the%20endocytosis%20of%20transferrin%20receptors.%20Knock%20down%20of%20endogenous%20SMAP1%20also%20abolished%20transferrin%20internalization,%20which%20confirms%20that%20SMAP1%20is%20needed%20for%20this%20endocytic%20process.%20SMAP1%20overexpression%20had%20no%20effect%20on%20clathrin-independent%20endocytosis,%20however.%20Intriguingly,%20SMAP1%20binds%20directly%20to%20the%20clathrin%20heavy%20chain%20via%20its%20clathrin-box%20and%20mutation%20studies%20revealed%20that%20its%20GAP%20domain%20and%20clathrin-box%20both%20contribute%20to%20the%20role%20SMAP1%20plays%20in%20clathrin-dependent%20endocytosis.%20These%20observations%20suggest%20that%20SMAP1%20may%20be%20an%20Arf6GAP%20that%20specifically%20regulates%20one%20of%20the%20multiple%20functions%20of%20Arf6,%20namely,%20clathrin-dependent%20endocytosis,%20and%20that%20it%20does%20so%20by%20binding%20directly%20to%20clathrin.}}

@article{Funaki,
	Author = {Funaki, Tomo and Kon, Shunsuke and Tanabe, Kenji and Natsume, Waka and Sato, Sayaka and Shimizu, Tadafumi and Yoshida, Naomi and Wong, Won Fen and Ogura, Atsuo and Ogawa, Takehiko and Inoue, Kimiko and Ogonuki, Narumi and Miki, Hiromi and Mochida, Keiji and Endoh, Keisuke and Yomogida, Kentarou and Fukumoto, Manabu and Horai, Reiko and Iwakura, Yoichiro and Ito, Chizuru and Toshimori, Kiyotaka and Watanabe, Toshio and Satake, Masanobu},
	Date = {2013/09/01},
	Date-Added = {2017-07-29 22:53:05 +0000},
	Date-Modified = {2017-07-29 22:53:05 +0000},
	Journal = {Molecular Biology of the Cell},
	M3 = {10.1091/mbc.E13-05-0234},
	Month = {09},
	Number = {17},
	Pages = {2633--2644},
	Title = {The Arf GAP SMAP2 is necessary for organized vesicle budding from the trans-Golgi network and subsequent acrosome formation in spermiogenesis},
	Ty = {JOUR},
	Url = {http://www.molbiolcell.org/content/24/17/2633.abstract N2 - The trans-Golgi network (TGN) functions as a hub organelle in the exocytosis of clathrin-coated membrane vesicles, and SMAP2 is an Arf GTPase-activating protein that binds to both clathrin and the clathrin assembly protein (CALM). In the present study, SMAP2 is detected on the TGN in the pachytene spermatocyte to the round spermatid stages of spermatogenesis. Gene targeting reveals that SMAP2-deficient male mice are healthy and survive to adulthood but are infertile and exhibit globozoospermia. In SMAP2-deficient spermatids, the diameter of proacrosomal vesicles budding from TGN increases, TGN structures are distorted, acrosome formation is severely impaired, and reorganization of the nucleus does not proceed properly. CALM functions to regulate vesicle sizes, and this study shows that CALM is not recruited to the TGN in the absence of SMAP2. Furthermore, syntaxin2, a component of the soluble N-ethylmaleimide–sensitive factor attachment protein receptor (SNARE) complex, is not properly concentrated at the site of acrosome formation. Thus this study reveals a link between SMAP2 and CALM/syntaxin2 in clathrin-coated vesicle formation from the TGN and subsequent acrosome formation. SMAP2-deficient mice provide a model for globozoospermia in humans.},
	Volume = {24},
	Year = {2013},
	Bdsk-Url-1 = {http://www.molbiolcell.org/content/24/17/2633.abstract%20N2%20-%20The%20trans-Golgi%20network%20(TGN)%20functions%20as%20a%20hub%20organelle%20in%20the%20exocytosis%20of%20clathrin-coated%20membrane%20vesicles,%20and%20SMAP2%20is%20an%20Arf%20GTPase-activating%20protein%20that%20binds%20to%20both%20clathrin%20and%20the%20clathrin%20assembly%20protein%20(CALM).%20In%20the%20present%20study,%20SMAP2%20is%20detected%20on%20the%20TGN%20in%20the%20pachytene%20spermatocyte%20to%20the%20round%20spermatid%20stages%20of%20spermatogenesis.%20Gene%20targeting%20reveals%20that%20SMAP2-deficient%20male%20mice%20are%20healthy%20and%20survive%20to%20adulthood%20but%20are%20infertile%20and%20exhibit%20globozoospermia.%20In%20SMAP2-deficient%20spermatids,%20the%20diameter%20of%20proacrosomal%20vesicles%20budding%20from%20TGN%20increases,%20TGN%20structures%20are%20distorted,%20acrosome%20formation%20is%20severely%20impaired,%20and%20reorganization%20of%20the%20nucleus%20does%20not%20proceed%20properly.%20CALM%20functions%20to%20regulate%20vesicle%20sizes,%20and%20this%20study%20shows%20that%20CALM%20is%20not%20recruited%20to%20the%20TGN%20in%20the%20absence%20of%20SMAP2.%20Furthermore,%20syntaxin2,%20a%20component%20of%20the%20soluble%20N-ethylmaleimide%E2%80%93sensitive%20factor%20attachment%20protein%20receptor%20(SNARE)%20complex,%20is%20not%20properly%20concentrated%20at%20the%20site%20of%20acrosome%20formation.%20Thus%20this%20study%20reveals%20a%20link%20between%20SMAP2%20and%20CALM/syntaxin2%20in%20clathrin-coated%20vesicle%20formation%20from%20the%20TGN%20and%20subsequent%20acrosome%20formation.%20SMAP2-deficient%20mice%20provide%20a%20model%20for%20globozoospermia%20in%20humans.}}

@article{Natsume,
	Author = {Natsume, Waka and Tanabe, Kenji and Kon, Shunsuke and Yoshida, Naomi and Watanabe, Toshio and Torii, Tetsuo and Satake, Masanobu},
	Date = {2006/06/01},
	Date-Added = {2017-07-29 22:45:08 +0000},
	Date-Modified = {2017-07-29 22:45:49 +0000},
	Journal = {Molecular Biology of the Cell},
	M3 = {10.1091/mbc.E05-10-0909},
	Month = {06},
	Number = {6},
	Pages = {2592--2603},
	Title = {{SMAP}2, a Novel {ARF} {GTP}ase-activating Protein, Interacts with Clathrin and Clathrin Assembly Protein and Functions on the {AP}-1--positive Early Endosome/Trans-{G}olgi Network.},
	Ty = {JOUR},
	Url = {http://www.molbiolcell.org/content/17/6/2592.abstract N2 - We recently reported that SMAP1, a GTPase-activating protein (GAP) for Arf6, directly interacts with clathrin and regulates the clathrin-dependent endocytosis of transferrin receptors from the plasma membrane. Here, we identified a SMAP1 homologue that we named SMAP2. Like SMAP1, SMAP2 exhibits GAP activity and interacts with clathrin heavy chain (CHC). Furthermore, we show that SMAP2 interacts with the clathrin assembly protein CALM. Unlike SMAP1, however, SMAP2 appears to be a regulator of Arf1 in vivo, because cells transfected with a GAP-negative SMAP2 mutant were resistant to brefeldin A. SMAP2 colocalized with the adaptor proteins for clathrin AP-1 and EpsinR on the early endosomes/trans-Golgi-network (TGN). Moreover, overexpression of SMAP2 delayed the accumulation of TGN38/46 molecule on the TGN. This suggests that SMAP2 functions in the retrograde, early endosome-to-TGN pathway in a clathrin- and AP-1–dependent manner. Thus, the SMAP gene family constitutes an important ArfGAP subfamily, with each SMAP member exerting both common and distinct functions in vesicle trafficking.},
	Volume = {17},
	Year = {2006},
	Bdsk-Url-1 = {http://www.molbiolcell.org/content/17/6/2592.abstract%20N2%20-%20We%20recently%20reported%20that%20SMAP1,%20a%20GTPase-activating%20protein%20(GAP)%20for%20Arf6,%20directly%20interacts%20with%20clathrin%20and%20regulates%20the%20clathrin-dependent%20endocytosis%20of%20transferrin%20receptors%20from%20the%20plasma%20membrane.%20Here,%20we%20identified%20a%20SMAP1%20homologue%20that%20we%20named%20SMAP2.%20Like%20SMAP1,%20SMAP2%20exhibits%20GAP%20activity%20and%20interacts%20with%20clathrin%20heavy%20chain%20(CHC).%20Furthermore,%20we%20show%20that%20SMAP2%20interacts%20with%20the%20clathrin%20assembly%20protein%20CALM.%20Unlike%20SMAP1,%20however,%20SMAP2%20appears%20to%20be%20a%20regulator%20of%20Arf1%20in%20vivo,%20because%20cells%20transfected%20with%20a%20GAP-negative%20SMAP2%20mutant%20were%20resistant%20to%20brefeldin%20A.%20SMAP2%20colocalized%20with%20the%20adaptor%20proteins%20for%20clathrin%20AP-1%20and%20EpsinR%20on%20the%20early%20endosomes/trans-Golgi-network%20(TGN).%20Moreover,%20overexpression%20of%20SMAP2%20delayed%20the%20accumulation%20of%20TGN38/46%20molecule%20on%20the%20TGN.%20This%20suggests%20that%20SMAP2%20functions%20in%20the%20retrograde,%20early%20endosome-to-TGN%20pathway%20in%20a%20clathrin-%20and%20AP-1%E2%80%93dependent%20manner.%20Thus,%20the%20SMAP%20gene%20family%20constitutes%20an%20important%20ArfGAP%20subfamily,%20with%20each%20SMAP%20member%20exerting%20both%20common%20and%20distinct%20functions%20in%20vesicle%20trafficking.}}

