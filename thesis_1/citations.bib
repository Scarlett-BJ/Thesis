@article{Kim,
	Annote = {10.1073/pnas.1406459111},
	Author = {Kim, Dae In and KC, Birendra and Zhu, Wenhong and Motamedchaboki, Khatereh and Doye, Val{\'e}rie and Roux, Kyle J.},
	Date = {2014/06/17},
	Date-Added = {2017-07-19 18:47:04 +0000},
	Date-Modified = {2017-07-19 18:47:11 +0000},
	Journal = {Proceedings of the National Academy of Sciences},
	Journal1 = {Proceedings of the National Academy of Sciences},
	Month = {06},
	Number = {24},
	Pages = {E2453--E2461},
	Title = {Probing nuclear pore complex architecture with proximity-dependent biotinylation.},
	Ty = {JOUR},
	Url = {http://www.pnas.org/content/111/24/E2453.abstract N2 - Proximity-dependent biotin identification (BioID) is a method for identifying protein associations that occur in vivo. By fusing a promiscuous biotin ligase to a protein of interest expressed in living cells, BioID permits the labeling of proximate proteins during a defined labeling period. In this study we used BioID to study the human nuclear pore complex (NPC), one of the largest macromolecular assemblies in eukaryotes. Anchored within the nuclear envelope, NPCs mediate the nucleocytoplasmic trafficking of numerous cellular components. We applied BioID to constituents of the Nup107–160 complex and the Nup93 complex, two conserved NPC subcomplexes. A strikingly different set of NPC constituents was detected depending on the position of these BioID-fusion proteins within the NPC. By applying BioID to several constituents located throughout the extremely stable Nup107–160 subcomplex, we refined our understanding of this highly conserved subcomplex, in part by demonstrating a direct interaction of Nup43 with Nup85. Furthermore, by using the extremely stable Nup107–160 structure as a molecular ruler, we defined the practical labeling radius of BioID. These studies further our understanding of human NPC organization and demonstrate that BioID is a valuable tool for exploring the constituency and organization of large protein assemblies in living cells.},
	Volume = {111},
	Year = {2014},
	Bdsk-Url-1 = {http://www.pnas.org/content/111/24/E2453.abstract%20N2%20-%20Proximity-dependent%20biotin%20identification%20(BioID)%20is%20a%20method%20for%20identifying%20protein%20associations%20that%20occur%20in%20vivo.%20By%20fusing%20a%20promiscuous%20biotin%20ligase%20to%20a%20protein%20of%20interest%20expressed%20in%20living%20cells,%20BioID%20permits%20the%20labeling%20of%20proximate%20proteins%20during%20a%20defined%20labeling%20period.%20In%20this%20study%20we%20used%20BioID%20to%20study%20the%20human%20nuclear%20pore%20complex%20(NPC),%20one%20of%20the%20largest%20macromolecular%20assemblies%20in%20eukaryotes.%20Anchored%20within%20the%20nuclear%20envelope,%20NPCs%20mediate%20the%20nucleocytoplasmic%20trafficking%20of%20numerous%20cellular%20components.%20We%20applied%20BioID%20to%20constituents%20of%20the%20Nup107%E2%80%93160%20complex%20and%20the%20Nup93%20complex,%20two%20conserved%20NPC%20subcomplexes.%20A%20strikingly%20different%20set%20of%20NPC%20constituents%20was%20detected%20depending%20on%20the%20position%20of%20these%20BioID-fusion%20proteins%20within%20the%20NPC.%20By%20applying%20BioID%20to%20several%20constituents%20located%20throughout%20the%20extremely%20stable%20Nup107%E2%80%93160%20subcomplex,%20we%20refined%20our%20understanding%20of%20this%20highly%20conserved%20subcomplex,%20in%20part%20by%20demonstrating%20a%20direct%20interaction%20of%20Nup43%20with%20Nup85.%20Furthermore,%20by%20using%20the%20extremely%20stable%20Nup107%E2%80%93160%20structure%20as%20a%20molecular%20ruler,%20we%20defined%20the%20practical%20labeling%20radius%20of%20BioID.%20These%20studies%20further%20our%20understanding%20of%20human%20NPC%20organization%20and%20demonstrate%20that%20BioID%20is%20a%20valuable%20tool%20for%20exploring%20the%20constituency%20and%20organization%20of%20large%20protein%20assemblies%20in%20living%20cells.}}

