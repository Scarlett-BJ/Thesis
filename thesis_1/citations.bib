@article{Mu,
	Author = {Mu, Fi-Tjen and Callaghan, Judy M. and Steele-Mortimer, Olivia and Stenmark, Harald and Parton, Robert G. and Campbell, Paul L. and McCluskey, James and Yeo, Jing-Ping and Tock, Edward P. C. and Toh, Ban-Hock},
	Date = {1995/06/02},
	Date-Added = {2017-07-23 21:46:30 +0000},
